{
  "name": "ai-caramba",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "eslint_check": "next lint",
    "stylelint_check": "stylelint 'src/**/*.{scss,*.scss,css,*.css,*.scss.d.ts,.scss.d.ts}'",
    "type_watch": "tsc --project . --watch",
    "type_check": "tsc --project .",
    "prettier_check": "prettier --check 'src/**/*.{js,*.js,ts,*.ts,tsx,*.tsx,scss,*.scss,css,*.css,json,*.scss.d.ts,.scss.d.ts}'",
    "check_all": "npm run prettier_check && npm run eslint_check && npm run stylelint_check && npm run type_check",
    "eslint_fix": "next lint 'src/**/*.{js,*.js,ts,*.ts,tsx,*.tsx}' --fix",
    "stylelint_fix": "stylelint 'src/**/*.{scss,*.scss,css,*.css,*.scss.d.ts,.scss.d.ts}' --fix",
    "prettier_fix": "prettier --write 'src/**/*.{js,*.js,ts,*.ts,tsx,*.tsx,scss,*.scss,css,*.css,json,*.scss.d.ts,.scss.d.ts}'",
    "pre-commit": "lint-staged",
    "postinstall": "npx prisma generate"
  },
  "lint-staged": {
    "src/**/*.{js,*.js,ts,*.ts,tsx,*.tsx,json,*.scss.d.ts,.scss.d.ts}": [
      "prettier --write",
      "eslint --fix"
    ],
    "src/**/*.{scss,*.scss,css,*.css,*.scss.d.ts,.scss.d.ts}": [
      "stylelint --fix"
    ]
  },
  "prisma": {
    "schema": "prisma/schema.prisma",
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed/index.ts"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.2",
    "@fortawesome/free-brands-svg-icons": "^6.1.2",
    "@fortawesome/free-regular-svg-icons": "^6.1.2",
    "@fortawesome/free-solid-svg-icons": "^6.1.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@mantine/core": "^5.2.4",
    "@mantine/hooks": "^5.2.4",
    "@next/font": "^13.0.6",
    "@prisma/client": "^4.8.0",
    "@tanstack/react-query": "^4.20.4",
    "@trpc/client": "^10.7.0",
    "@trpc/next": "^10.7.0",
    "@trpc/react-query": "^10.7.0",
    "@trpc/server": "^10.7.0",
    "@types/node": "18.11.10",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "eslint": "8.28.0",
    "eslint-config-next": "13.0.5",
    "jsonwebtoken": "^9.0.0",
    "next": "13.0.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "4.9.3",
    "zod": "^3.20.2",
    "zustand": "^4.1.5"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^3.3.0",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "lint-staged": "^13.0.4",
    "prettier": "^2.7.1",
    "prisma": "^4.8.0",
    "sass": "^1.56.1",
    "stylelint": "^14.15.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-scss": "^7.0.0",
    "stylelint-config-standard": "^28.0.0",
    "stylelint-prettier": "^2.0.0",
    "stylelint-scss": "^4.3.0",
    "stylelint-webpack-plugin": "^3.3.0",
    "ts-node": "^10.9.1"
  }
}
